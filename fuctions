def is_prime_number(n):
    if n <= 1:
        return False
    if n == 2:
        return True
    for i in range (3, int(n ** 0.5) + 1):
        if n % 2 == 1:
            return True
n = int(input("Enter a number: "))
if is_prime_number(n):
                print(f"{n} is a prime number")
else:
                print(f"{n} is not a prime number")


    